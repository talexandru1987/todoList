  <%- include("header") -%>
    <div class="box" id="heading">
      <h1><%= listTitle %>!</h1>
    </div>

    <div class="box">
      <%for( let i = 0; i < listItemes.length; i++){%>

      <div class="item">
        <input type="checkbox">
        <p><%= listItemes[i].name %></div></p>
 
      <%}%>

      <form class="item" action="/api" method="post">
        <input type="text" name="todoItem" placeholder="New Item" autocomplete="off"/>
        <button type="submit" name="list" value=<%= listTitle%>>+</button>
      </form>
    </div>
    <%- include("footer") -%>
    

